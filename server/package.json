{
  "name": "ez-tracking-board-server",
  "version": "1.0.0",
  "description": "Backend server for EZ Tracking Board",
  "main": "dist/tracking-server.js",
  "scripts": {
    "start": "node dist/tracking-server.js",
    "dev": "nodemon src/tracking-server.ts",
    "worker": "node dist/jobProcessor.js",
    "worker:dev": "nodemon src/jobProcessor.ts",
    "bull-board": "node dist/bullBoardServer.js",
    "bull-board:dev": "nodemon src/bullBoardServer.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "ts-node src/migrations/migrate.ts migrate",
    "migrate:rollback": "ts-node src/migrations/migrate.ts rollback",
    "migrate:status": "ts-node src/migrations/migrate.ts status",
    "migrate:create": "ts-node src/migrations/migrate.ts create",
    "migrate:prod": "node dist/migrations/migrate.js migrate"
  },
  "dependencies": {
    "@bull-board/api": "^6.12.6",
    "@bull-board/express": "^6.12.6",
    "@bull-board/ui": "^6.12.6",
    "@types/pg": "^8.15.5",
    "axios": "^1.6.2",
    "bullmq": "^5.58.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/sqlite3": "^3.1.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
